<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.scada.domain.EleCase" table="elecase">
		
		<id name="id" type="java.lang.Integer">
			<column name="ID" />
			<generator class="native" />
		</id>
		<property name="user" type="java.lang.String">
			<column name="user"></column>
		</property>
		<property name="time" type="java.util.Date">
			<column name="time"></column>
		</property>
		<property name="action" type="java.lang.String">
			<column name="action"></column>
		</property>
		<property name="info" type="java.lang.String">
			<column name="info"></column>
		</property>
		<property name="type" type="java.lang.String">
			<column name="type"></column>
		</property>
	</class>
</hibernate-mapping>